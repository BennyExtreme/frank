{
  "name": "frank",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Java_S",
    "email": "java_s@qq.com"
  },
  "version": "1.0.0",
  "main": "dist/electron/main/index.js",
  "description": "A bran-new League of Legends assistant software, a replacement for WeGame.",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vite build && electron-builder"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3000
  },
  "dependencies": {
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vicons/tabler": "^0.12.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "echarts": "^5.3.3",
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-store": "^8.0.2",
    "league-connect": "6.0.0-rc4",
    "less": "^4.1.2",
    "naive-ui": "2.29.0",
    "path-browserify": "^1.0.1",
    "pinia": "^2.0.16",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-electron": "^0.7.5",
    "vue-echarts": "^6.2.3",
    "vue-tsc": "^0.38.4"
  },
  "build": {
    "appId": "com.electron.frank",
    "productName": "Frank",
    "asar": true,
    "copyright": "Copyright Â© 2022 Java_S",
    "extraResources": [
      {
        "from": "./resources",
        "to": "./resources"
      }
    ],
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "resources/frank.ico",
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    }
  }
}